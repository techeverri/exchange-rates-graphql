{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/ping",
      "dest": "/src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/graphql",
      "dest": "/src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/login",
      "dest": "/src/index.js",
      "methods": ["POST"]
    }
  ]
}
